{
  "name": "@storybook/aem",
  "version": "0.0.5",
  "description": "Storybook for AEM: View AEM components in isolation with hot reloading.",
  "keywords": [
    "storybook",
    "aem"
  ],
  "homepage": "https://github.com/storybookjs/aem/app/aem",
  "bugs": {
    "url": "https://github.com/storybookjs/aem/issues"
  },
  "repository": {
    "type": "git",
    "directory": "app/aem"
  },
  "license": "MIT",
  "files": [
    "bin/**/*",
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts",
    "ts3.5/**/*"
  ],
  "main": "dist/client/index.js",
  "typings": "dist/client/index.d.ts",
  "bin": {
